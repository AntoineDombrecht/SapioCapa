<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <add key="FTP_Protocol" value="ftp" />
    <add key="FTP_HostName" value="example.ftp.server.services" />
    <add key="FTP_Port" value="21" />
    <add key="FTP_UserName" value="john_doe" />
    <add key="FTP_Password" value="MyPassword" />
    <add key="FTP_SshHostKeyFingerprint" value="ssh-rsa 2048 xx:xx:xx:xx:xx:xx:xx:xx:..." />
    <add key="Mail_UserName" value="john.doe@example.com" />
    <add key="Mail_Password" value="MyPassword" />
    <add key="Mail_Server" value="https://email.example/EWS/Exchange.asmx" />
    <add key="WinSCP_ExecutablePath" value="\WinSCP.exe" />
    <add key="WinSCP_DebugLogLevel" value="2" />
    <add key="WinSCP_DebugLogPath" value="\winscp.log" />
    <add key="Local_TempPath" value="\temp\" />
    <add key="Local_XMLPath" value="\data-set.xml" /> 
    <add key="Local_XLSXPath" value="\data-set.xlsx" />
    <add key="Local_DebugPath" value="\output.log" />
    <add key="Program_DateTimeFormat" value="Mdyyyy hh-mm-ss" />
    <add key="Program_Col1" value="1" />
    <add key="Program_Col2" value="2" />
    <add key="Program_Col3" value="3" />
    <add key="Program_Col4" value="4" />
    <add key="Program_Col5" value="5" />
    <add key="Program_Col6" value="7" />
    <add key="Program_Col7" value="9" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>